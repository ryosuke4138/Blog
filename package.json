{
  "name": "okamae-blog",
  "version": "0.1.2",
  "description": "A fashionable brutalist blog template using Netlify CMS and Nuxt.js",
  "author": "jake101 <hello@jake101.com>",
  "scripts": {
    "dev": "nuxt",
    "serve": "nuxt",
    "build": "nuxt build --no-lock",
